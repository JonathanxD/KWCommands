{
  "name": "register",
  "description": "Registers the user",
  "handler": "method:register",
  "arguments": [
    {
      "id": "name",
      "type": "String"
    },
    {
      "id": "email",
      "type": "String",
      "validator": "EmailValidator"
    }
  ],
  "requiredInfo": [
